{
  "redirects": [
    {
      "source": "/blog/:path*",
      "destination": "https://blog.psikologduyguaksoy.com/:path*",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/image/:path*",
      "destination": "/image/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next/static|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
