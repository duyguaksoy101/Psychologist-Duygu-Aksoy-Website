{
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://https://blog-on-hashnode.vercel.app/blog"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://https://blog-on-hashnode.vercel.app//blog/:path*"
    }
  ]
}
